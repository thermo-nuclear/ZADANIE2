#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main()
{
    int i;
    int balance_round = 0;
    int balance_brace = 0;
    int flag = 0;
    char c[100000];
    scanf ("%s", c);
    int l = strlen (c);

    for (i = 0; i < l; i++)
        printf ("%c", c[i]);
    for (i = 0; i < l; i++)
    {
        if (c[i] == '(')
        {
            balance_round++;
            if (i != l)
                if (c[i + 1] == '}')
                    {
                        flag = 1;
                        break;
                    }
        }
        if (c[i] == ')')
            balance_round--;
        if (c[i] == '{')
        {
            balance_brace++;
            if (i != l)
                if (c[i + 1] == ')')
                    {
                        flag = 1;
                        break;
                    }
        }
        if (c[i] == '}')
            balance_brace--;
        if (balance_round < 0 || balance_brace < 0)
            break;
    }

    if (flag == 0 && balance_brace == 0 && balance_round == 0)
        printf ("Correct");
    else
        printf ("Wrong");
    return 0;
}
